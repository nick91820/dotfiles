[user]
	name = Nick J Moss
	email = 78372873+nickjmoss@users.noreply.github.com
[push]
	default = simple
[pull]
  rebase = true
[branch]
  autosetuprebase = always
[init]
	defaultBranch = main
[color]
	ui = true
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
[color "diff"]
	meta = 11
	frag = magenta bold
	commit = yellow bold
	old = red bold
	new = green bold
	whitespace = red reverse
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
